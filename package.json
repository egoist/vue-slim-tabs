{
  "name": "vue-slim-tabs",
  "version": "0.3.0",
  "description": "Simple tabs component for Vue.js",
  "repository": {
    "url": "egoist/vue-slim-tabs",
    "type": "git"
  },
  "module": "dist/vue-slim-tabs.esm.js",
  "main": "dist/vue-slim-tabs.cjs.js",
  "unpkg": "dist/vue-slim-tabs.js",
  "jsdelivr": "dist/vue-slim-tabs.js",
  "cdn": "dist/vue-slim-tabs.js",
  "types": "types/vue-slim-tabs.d.ts",
  "poi": {
    "entry": "example/index.js",
    "output": {
      "dir": "example/dist"
    }
  },
  "files": [
    "dist",
    "themes",
    "types"
  ],
  "scripts": {
    "test": "echo 'no tests!' && npm run lint",
    "prepublish": "npm run build",
    "lint": "xo",
    "build": "bili",
    "build:example": "poi --prod",
    "dev": "poi -so"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bili": "^4.8.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "poi": "^12.6.10",
    "prettier": "^1.18.2",
    "vue": "^2.6.10",
    "vue-github-badge": "^1.0.0",
    "vue-template-compiler": "^2.6.10",
    "xo": "^0.27.1"
  },
  "xo": {
    "extends": [
      "rem",
      "plugin:prettier/recommended"
    ],
    "ignores": [
      "example/**"
    ]
  }
}
